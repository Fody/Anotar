<?xml version="1.0" encoding="utf-8"?>
<Project>
  <PropertyGroup>
    <PackageVersion>4.0.0</PackageVersion>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <LangVersion>latest</LangVersion>
    <DebugType>full</DebugType>
    <Authors>Simon Cropp, Tomasz Pluskiewicz</Authors>
    <Description>Simplifies logging through a static class and some IL manipulation.</Description>
    <PackageTags>Logging, ILWeaving, Fody, Cecil</PackageTags>
    <AssemblyOriginatorKeyFile>$(SolutionDir)key.snk</AssemblyOriginatorKeyFile>
    <NoWarn>CS0618;NU1701</NoWarn>
  </PropertyGroup>
</Project>